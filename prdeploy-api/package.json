{
  "name": "github-prdeploy",
  "description": "Manages feature branch deployments across environments through a pull request.",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "npm-run-all --parallel build:watch run:watch",
    "webhook": "npx smee -u https://smee.io/3IVRinBfSv0D9bz -t http://localhost:3000/api/webhook",
    "start:prod": "npm run build && node dist/src/app.js",
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "run:watch": "nodemon dist/src/app.js --watch dist",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "cross-env USE_MOCKS=true jest",
    "checks": "npm-run-all lint build test"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/async-lock": "^1.4.2",
    "@types/jest": "^29.5.12",
    "@types/jira-client": "^7.1.9",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.202",
    "@types/memory-cache": "^0.2.5",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "smee-client": "^1.2.3",
    "standard": "^17.0.0",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.2",
    "vite": "^4.5.3"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.519.0",
    "@octokit/rest": "^20.0.2",
    "adm-zip": "^0.5.10",
    "async-lock": "^1.4.0",
    "dotenv": "^16.0.3",
    "handlebars": "^4.7.8",
    "jira-client": "^8.2.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "octokit": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "slack": "^11.0.2",
    "slackify-markdown": "github:greggbjensen/slackify-markdown#html-tag-handling",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.8.0",
    "yaml": "^2.3.4"
  }
}
